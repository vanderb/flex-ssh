{
    "name": "vanderb/flex-ssh",
    "description": "Mange ssh-aliases via terminal.",
    "keywords": ["ssh", "console", "cli"],
    "homepage": "https://github.com/vanderb/flex-ssh",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/vanderb/flex-ssh/issues",
        "source": "https://github.com/vanderb/flex-ssh"
    },
    "authors": [
        {
            "name": "Freerk van Zeijl",
            "email": "freerk.vz@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laminas/laminas-text": "^2.8"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^1.0",
        "laravel-zero/framework": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/flex-ssh"]
}
